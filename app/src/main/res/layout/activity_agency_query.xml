<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.Store.www.ui.activity.AgencyQueryActivity">

    <include layout="@layout/layout_toolbar"/>

    <EditText
        android:id="@+id/et_agency_number"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:hint="@string/login_hint"
        android:layout_marginRight="@dimen/drawablePadding_5dp"
        android:layout_marginLeft="@dimen/drawablePadding_5dp"
        android:background="@color/windowBackground"/>
    <View
        style="@style/divider_horizontal2"/>

    <Button
        android:id="@+id/btn_query"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:layout_marginLeft="@dimen/divider_10dp"
        android:layout_marginRight="@dimen/divider_10dp"
        android:layout_marginTop="@dimen/dp_40"
        android:background="@drawable/select_button_square"
        android:text="@string/query"
        android:textSize="@dimen/ysf_text_size_15"
        android:enabled="false"
        android:textColor="@color/windowBackgroundWhite"/>

</LinearLayout>
