<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <LinearLayout
        android:id="@+id/layout_agency_equity_hint"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:gravity="center"
        android:layout_gravity="center"
        android:paddingBottom="@dimen/divider_10dp">

        <ImageView
            android:id="@+id/iv_agency_equity_icon"
            android:layout_width="@dimen/divider_30dp"
            android:layout_height="@dimen/divider_30dp"
            android:src="@mipmap/jzz_img"/>

        <TextView
            android:id="@+id/tv_agency_equity_content"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/medium_text_size"
            android:text="招收代理"/>


    </LinearLayout>

</LinearLayout>